@page
@using ClassDemoKaffeWebApp.model;
@model ClassDemoKaffeWebApp.Pages.Kunder.IndexModel
@{
}

<h1 class="jumbotron">Vis Alle Kunder</h1>

<table class="table table-hover">
    <thead>
        <tr>
            <th>Kunde Nummer</th>
            <th>Navn</th>
            <th>Telefon</th>
        </tr>
    </thead>
    <tbody>
        @foreach(Kunde k in Model.Kunder)
        {
            <tr>
                <td>@k.KundeNummer</td>
                <td>@k.Navn</td>
                <td>@k.Tlf</td>
            </tr>
        }
    </tbody>
    </table>

    <p>That was it</p>




</table>


